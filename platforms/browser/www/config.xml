<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.mirprice" version="1.1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>MirPrice</name>
    <description>
        mirprice
    </description>
    <author email="al.fedyakin@gmail.com">
        al.fedyakin
    </author>
    <icon src="res/icon/mirprice.png" />
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="*://*.mirprice.com/*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
